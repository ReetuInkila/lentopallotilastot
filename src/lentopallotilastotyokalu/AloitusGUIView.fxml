            <Label text="Lentopallo tilastotyökalu">
               <font>
                  <Font size="34.0" />
               </font>
               <VBox.margin>
                  <Insets left="130.0" right="130.0" top="150.0" />
               </VBox.margin>
            </Label>
            <HBox prefHeight="297.0" prefWidth="184.0">
               <children>
                  <VBox prefHeight="643.0" prefWidth="285.0">
                     <children>
                        <ListChooser rivit="&#10;Puulaaki&#10;Karvapallot&#10;Valepa">
                           <VBox.margin>
                              <Insets left="10.0" />
                           </VBox.margin>
                        </ListChooser>
                        <HBox prefHeight="100.0" prefWidth="200.0">
                           <children>
                              <Button mnemonicParsing="false" onAction="#onHandleLisaaJoukkue" text="Lisää joukkue">
